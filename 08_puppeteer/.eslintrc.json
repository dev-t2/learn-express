{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "linebreak-style": "off",
    "no-console": "off",
    "comma-dangle": "off",
    "no-plusplus": "off",
    "no-await-in-loop": "off"
  }
}
